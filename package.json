    "version": "0.0.0",
    "module": "./dist/index.js",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "- BUILD -------------------------------------------": "echo 'unused'",
        "build": "npm run clean:build && tsc -p ./tsconfig.json && node ./esbuild.config.js",
        "build:test": "tsc -p ./tsconfig.test.json",
        "- CLEAN -------------------------------------------": "echo 'unused'",
        "clean": "npm run clean:build && rimraf ./node_modules",
        "clean:build": "rimraf ./dist-test && rimraf ./dist",
        "- CHECK -------------------------------------------": "echo 'unused'",
        "check": "npm run check:eslint && npm run check:cspell && npm run check:tsc && npm run check:importsort",
        "check:eslint": "eslint ./src/**/*.{ts,tsx}",
        "check:cspell": "cspell src/**/*.ts src/**/*.html src/**/*.less src/**/*.sass src/**/*.css",
        "check:tsc": "tsc --noEmit --emitDeclarationOnly false",
        "check:importsort": "import-sort -l ./src/**/*.{ts,tsx}",
        "- FIX ---------------------------------------------": "echo 'unused'",
        "fix": "npm run fix:eslint && npm run fix:importsort",
        "fix:eslint": "eslint --fix ./src/**/*.{ts,tsx}",
        "fix:importsort": "import-sort --write ./src/**/*.{ts,tsx}"
    },
    "dependencies": {
        "fp-ts": "2.11.8",
        "io-ts": "2.2.16",
        "rxjs": "7.5.5",
        "@morphic-ts/adt": "3.0.0",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.14.0",

        "cspell": "5.18.5",
        "jshint": "2.13.4",
        "rimraf": "3.0.2",
        "typescript": "4.6.2",
        "esbuild": "0.14.25",
        "import-sort": "6.0.0",
        "import-sort-cli": "6.0.0",
        "import-sort-parser-typescript": "6.0.0",
        "esbuild-node-externals": "1.4.1",
        "type-fest": "2.12.0"
    }
}
